{
  "name": "vybot",
  "version": "0.0.1",
  "description": "This project contains all of the code required to run an instance of VyBot.",
  "main": "./dist/app.js",
  "scripts": {
    "build": "npm run clean && tsc",
    "build:watch": "npm run clean && tsc --watch",
    "clean": "rimraf ./dist",
    "database:setup": "node ./data/setup.js",
    "deploy": "sh ./bin/deploy.sh",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "start": "node ./dist/app.js",
    "test": "jest --silent",
    "test:ci": "jest --silent --maxWorkers=2",
    "test:log": "jest",
    "test:watch": "jest -w --silent",
    "lint": "eslint . --ext ts --ext js",
    "lint-fix": "eslint . --ext ts --ext js --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vyserion/vybot.git"
  },
  "author": "James Smith",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Vyserion/vybot/issues"
  },
  "homepage": "https://github.com/Vyserion/vybot#readme",
  "dependencies": {
    "discord.js": "12.2.0",
    "dotenv": "8.2.0",
    "npm": "6.13.7",
    "pg": "7.18.2",
    "winston": "3.2.1",
    "zlib-sync": "0.1.6"
  },
  "devDependencies": {
    "@types/dotenv": "8.2.0",
    "@types/jest": "25.1.3",
    "@types/node": "13.7.4",
    "@types/pg": "7.14.1",
    "@typescript-eslint/eslint-plugin": "2.19.0",
    "@typescript-eslint/parser": "2.19.0",
    "eslint": "6.8.0",
    "eslint-cli": "1.1.1",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "^4.2.3",
    "jest": "25.1.0",
    "lint-staged": "^10.0.7",
    "prettier": "1.19.1",
    "rimraf": "3.0.2",
    "ts-jest": "25.2.1",
    "typescript": "3.8.2"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>src/test/jest-config.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js}": "eslint --cache --fix"
  }
}
