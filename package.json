{
  "name": "vybot",
  "version": "0.0.1",
  "description": "This project contains all of the code required to run an instance of VyBot.",
  "main": "./dist/app.js",
  "scripts": {
    "build": "npm run clean && tsc",
    "build:watch": "npm run clean && tsc --watch",
    "clean": "rimraf ./dist",
    "start": "node ./dist/app.js",
    "test": "mocha ./dist/**/*.spec.js",
    "test:watch": "mocha -w ./dist/**/*.spec.js",
    "tidy": "npm run clean && prettier --write **/*.ts app.ts && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vyserion/vybot.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Vyserion/vybot/issues"
  },
  "homepage": "https://github.com/Vyserion/vybot#readme",
  "dependencies": {
    "bufferutil": "4.0.1",
    "discord.js": "11.4.2",
    "dotenv": "^6.0.0",
    "erlpack": "github:discordapp/erlpack",
    "libsodium-wrappers": "^0.7.3",
    "npm": "6.6.0-next.1",
    "pg": "^7.4.1",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.3",
    "sinon": "7.2.2",
    "uws": "99.0.0",
    "winston": "3.1.0",
    "zlib-sync": "^0.1.4"
  },
  "devDependencies": {
    "@types/dotenv": "6.1.0",
    "@types/node": "10.12.18",
    "@types/pg": "^7.4.2",
    "@types/sinon": "7.0.3",
    "@types/chai": "^4.1.2",
    "@types/mocha": "5.2.5",
    "chai": "4.2.0",
    "mocha": "5.2.0",
    "ts-mockito": "2.3.1"
  }
}
